{
  "expo": {
    "name": "Gossipify",
    "slug": "gossipify",
    "version": "0.1.0",
    "orientation": "portrait",
    
    "scheme": "gossipify",
    "userInterfaceStyle": "automatic",
    
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#0b1020"
      }
    },
    "ios": {
      "supportsTablet": false
    },
    "plugins": [
      ["expo-secure-store"],
      ["expo-document-picker"],
      ["expo-file-system"]
    ]
  }
}


